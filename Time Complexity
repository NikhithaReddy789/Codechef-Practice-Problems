import java.util.*;
import java.lang.*;
import java.io.*;

/* Code by Nikhitha Singireddy
   Code written in java */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner IP=new Scanner(System.in);
		int T=IP.nextInt();
		for(int i=0;i<T;i++){
		    int AC=IP.nextInt();
		    int BC=IP.nextInt();
		    if(AC>BC){
		        System.out.println("YES");
		    }
		    else{
		        System.out.println("NO");
		    }
		}
		IP.close();
	}
}
